#!/sbin/sh

nProp=/tmp/vanir-l5.prop

#copy .vanir-l5.prop
rm -f $nProp
cp -f /sdcard/.vanir-l5.prop $nProp

#create vanir-l5.prop if it doesn't exist
[ ! -f $nProp ] && touch $nProp

#check if kernel key is available and if not
#request the standard kernel
if (! grep -qi "kernel=" $nProp ); then
    echo "kernel=standard" >> $nProp
fi

