#!/sbin/sh

nProp=/tmp/vanir_nozomi.prop

#copy .vanir_nozomi.prop
rm -f $nProp
cp -f /sdcard/.vanir_nozomi.prop $nProp

#create vanir_nozomi.prop if it doesn't exist
[ ! -f $nProp ] && touch $nProp

#check if kernel key is available and if not
#request the standard kernel
if (! grep -qi "kernel=" $nProp ); then
    echo "kernel=standard" >> $nProp
fi

